[ ! -f "$rootfs/bin/clover-canoe-shvc-wr-o" ] && mv -f "$rootfs/bin/clover-canoe-shvc-wr" "$rootfs/bin/clover-canoe-shvc-wr-o"
copy "$transferpath/7zr" "$rootfs/bin/"
copy "$transferpath/snes-save-compress" "$rootfs/bin/"
copy "$transferpath/wait-for-exit" "$rootfs/bin/"
copy "$transferpath/clover-canoe-shvc-wr-scompress" "$rootfs/bin/"
copy "$transferpath/decompress-canoe-saves" "$rootfs/bin/"
copy "$transferpath/pacbd_move_compress_script" "$rootfs/etc/preinit.d/"
copy "$transferpath/S8101compress_saves" "$rootfs/etc/init.d/"
copy "$transferpath/compress_message0.raw.gz" "$rootfs/etc/"
copy "$transferpath/compress_message1.raw.gz" "$rootfs/etc/"
copy "$transferpath/compress_message2.raw.gz" "$rootfs/etc/"
chmod +x "$rootfs/bin/7zr"
chmod +x "$rootfs/bin/snes-save-compress"
chmod +x "$rootfs/bin/wait-for-exit"
chmod +x "$rootfs/bin/clover-canoe-shvc-wr-scompress"
chmod +x "$rootfs/bin/decompress-canoe-saves"
chmod +x "$rootfs/etc/init.d/S8101compress_saves"
return 1
