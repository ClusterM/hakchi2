mv -f "$rootfs/bin/clover-canoe-shvc-wr" "$rootfs/bin/clover-canoe-shvc-wr-o"
copy "$transferpath/7zr" "$rootfs/bin/"
copy "$transferpath/snes-save-compress" "$rootfs/bin/"
copy "$transferpath/wait-for-exit" "$rootfs/bin/"
copy "$transferpath/clover-canoe-shvc-wr-scompress" "$rootfs/bin/"
copy "$transferpath/pacbd_move_compress_script" "$rootfs/etc/preinit.d/"
chmod +x "$rootfs/bin/7zr"
chmod +x "$rootfs/bin/snes-save-compress"
chmod +x "$rootfs/bin/wait-for-exit"
chmod +x "$rootfs/bin/clover-canoe-shvc-wr-scompress"
# Compress all existing saves
find /var/lib/clover/profiles/0 -type d -name "rollback" | while read line; do
	if [ -f "$line/0.break" ] && [ ! -f "$line/rollback.7z" ]; then
		cd "$line" && 7zr u -mx9 rollback.7z *.inputs *.break && rm "$line"/*.inputs && rm "$line"/*.break
	fi
done
return 1
