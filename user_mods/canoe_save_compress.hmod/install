chmod +x "$rootfs/bin/7zr"
chmod +x "$rootfs/bin/snes-save-compress"
chmod +x "$rootfs/bin/wait-for-exit"
chmod +x "$rootfs/bin/clover-canoe-shvc-wr"
return 1